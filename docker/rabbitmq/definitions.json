{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "product_name": "RabbitMQ",
  "product_version": "3.12.0",
  "users": [
    {
      "name": "vita_admin",
      "password_hash": "Qk+XbkHGu3nM9NJ3GzXfN/+V+9LYzQ==",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "vita_vhost"
    }
  ],
  "permissions": [
    {
      "user": "vita_admin",
      "vhost": "vita_vhost",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "vita-cluster"
    }
  ],
  "policies": [
    {
      "vhost": "vita_vhost",
      "name": "vita-policy",
      "pattern": "vita.*",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic"
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "vita_tasks",
      "vhost": "vita_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000
      }
    },
    {
      "name": "vita_notifications",
      "vhost": "vita_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "vita_analytics",
      "vhost": "vita_vhost",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "exchanges": [
    {
      "name": "vita_exchange",
      "vhost": "vita_vhost",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "vita_exchange",
      "vhost": "vita_vhost",
      "destination": "vita_tasks",
      "destination_type": "queue",
      "routing_key": "task.*",
      "arguments": {}
    },
    {
      "source": "vita_exchange",
      "vhost": "vita_vhost",
      "destination": "vita_notifications",
      "destination_type": "queue",
      "routing_key": "notification.*",
      "arguments": {}
    },
    {
      "source": "vita_exchange",
      "vhost": "vita_vhost",
      "destination": "vita_analytics",
      "destination_type": "queue",
      "routing_key": "analytics.*",
      "arguments": {}
    }
  ]
}